{
  "name": "infinite-pokedex",
  "version": "1.0.0",
  "description": "Progressive Web App that generates AI-powered Pokémon lore",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:e2e": "playwright test",
    "test:coverage": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:security": "eslint . --ext .js --no-eslintrc --config .eslintrc.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test:all": "./scripts/test-suite.sh",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "@mlc-ai/web-llm": "^0.2.0",
    "axios": "^1.12.2",
    "cheerio": "^1.1.2",
    "puppeteer": "^24.23.0",
    "sharp": "^0.34.4"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@playwright/test": "^1.40.0",
    "eslint": "^8.0.0",
    "eslint-plugin-security": "^3.0.1",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.0.0",
    "vite": "^5.0.0"
  },
  "keywords": [
    "pokedex",
    "pwa",
    "pokemon",
    "ai",
    "webllm",
    "websd"
  ],
  "author": "Infinite Pokédex Team",
  "license": "MIT"
}
